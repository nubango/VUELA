    <dialogue-node nodeindex="12" editor-x="567" editor-y="820">
      <speak-char idTarget="invisible">
        <resources name="No name" />
        <text>Incorrecto. Las armas están prohibidas en los equipajes de mano o de cabina, incluso con licencia.</text>
      </speak-char>
      <condition />
