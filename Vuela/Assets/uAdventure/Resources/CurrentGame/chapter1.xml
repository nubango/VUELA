  <graph-conversation id="Fallo">
    <option-node nodeindex="0" preListening="yes" editor-x="0" editor-y="2" x="0" y="2">
      <speak-player>
        <text>Falta documentación</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="1" />
      <speak-player>
        <text>Existen discrepancias entre los datos de su documentación</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="2" />
      <speak-player>
        <text>Hay un objeto peligroso en el equipaje de mano</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="3" />
      <speak-player>
        <text>Queda menos de una hora antes de que se inicie el vuelo</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="4" />
    </option-node>
    <dialogue-node nodeindex="1" editor-x="695" editor-y="21">
      <speak-char idTarget="invisible">
        <resources name="No name" />
        <text>La tengo en la maleta, espere un segundo...</text>
      </speak-char>
      <condition>
        <equals var="Escena" value="3" />
      </condition>
      <speak-player>
        <resources name="No name" />
        <text>Lo lamento, pero tendrá que ir al final de la cola.</text>
      </speak-player>
      <condition>
        <equals var="Escena" value="3" />
      </condition>
      <speak-char idTarget="invisible">
        <resources name="No name" />
        <text>No, lo he traído todo.</text>
      </speak-char>
      <condition>
        <not-equals var="Escena" value="3" />
      </condition>
      <end-conversation>
        <effect>
          <trigger-cutscene idTarget="siguiente4" />
          <condition>
            <equals var="Escena" value="3" />
          </condition>
        </effect>
      </end-conversation>
    </dialogue-node>
    <dialogue-node nodeindex="2" editor-x="52" editor-y="382">
      <speak-char idTarget="invisible">
        <resources name="No name" />
        <text>Todos los datos bien.</text>
      </speak-char>
      <condition>
        <either>
          <not-equals var="Escena" value="5" />
          <not-equals var="Escena" value="7" />
        </either>
      </condition>
      <end-conversation />
    </dialogue-node>
    <dialogue-node nodeindex="3" editor-x="316" editor-y="341">
      <speak-player>
        <resources name="No name" />
        <text>No pueden introducirse tijeras de ese tamaño en el equipaje de mano.</text>
      </speak-player>
      <condition>
        <equals var="Escena" value="1" />
      </condition>
      <speak-player>
        <resources name="No name" />
        <text>No pueden introducirse botellas de más de 100 ml en el equipaje de mano.</text>
      </speak-player>
      <condition>
        <equals var="Escena" value="4" />
      </condition>
      <speak-char idTarget="invisible">
        <resources name="No name" />
        <text>No llevo nada peligroso.</text>
      </speak-char>
      <condition>
        <either>
          <not-equals var="Escena" value="1" />
          <not-equals var="Escena" value="4" />
        </either>
      </condition>
