  <graph-conversation id="Correcto">
    <dialogue-node nodeindex="0" editor-x="50" editor-y="50">
      <speak-player>
        <resources name="No name" />
        <text>Todo correcto, puede continuar.</text>
      </speak-player>
      <condition>
