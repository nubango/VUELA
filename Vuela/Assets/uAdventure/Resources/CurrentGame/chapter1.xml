    <option-node nodeindex="5" random="yes" keepShowing="yes" preListening="yes" editor-x="278" editor-y="420" x="278" y="420">
      <speak-player>
        <text>En el aeropuerto, antes del vuelo.</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="6" />
      <speak-player>
        <text>En el país de destino.</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="7" />
      <speak-player>
        <text>En nuestro banco antes del viaje.</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="8" />
      <speak-player>
        <text>En el avión, con otros pasajeros.</text>
