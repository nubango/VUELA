    <dialogue-node nodeindex="4" editor-x="262" editor-y="258">
      <speak-char idTarget="ninyaTest">
        <resources name="No name" />
        <text>¡No, hombre! Las puertas se sellan por la diferencia de presión entre el interior y el exterior.</text>
      </speak-char>
      <condition />
      <speak-char idTarget="ninyaTest">
        <resources name="No name" />
        <text>¡Ni siquiera podrían abrirse!</text>
      </speak-char>
      <condition />
      <child nodeindex="6" />
    </dialogue-node>
    <dialogue-node nodeindex="5" editor-x="267" editor-y="459">
